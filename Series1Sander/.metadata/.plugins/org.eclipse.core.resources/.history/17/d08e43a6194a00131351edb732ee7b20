
public class Main {

	public static void main(String[] args) {
		String msg = getMessage();
		System.out.println(msg);
	}
	
	private static String getMessage() {
		switch (getNum()) {
		case 0:
			return "Hello 0";
			break;
		case 1:
			return "Hello 1";
		default:
			return "Hello something else";
			break;
		}
	}

	private static int getNum() {
		return 2;
	}
}
